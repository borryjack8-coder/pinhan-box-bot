{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "webapp/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/bot(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/webapp/$1"
        }
    ]
}